   "source": [
    "row = [2,2,4,8]\n",
    "row = [x for x in row if x!=0 ]\n",
    "\n",
    "i=0\n",
    "N = len(row)\n",
    "while i < N-1:\n",
    "    if row[i] == row[i+1]:\n",
    "        row[i] *= 2\n",
    "        del row[i+1]\n",
    "        N-=1\n",
    "    else: i+=1\n",
    "        \n",
    "print(row + [0]*(4-N))"
   ]
  }
